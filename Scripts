local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

OrionLib:MakeNotification({
	Name = "secret hub v3",
	Content = "Welcome",
	Image = "rbxassetid://7733955740",
	Time = 5
})

local Window = OrionLib:MakeWindow({Name = "secret hub v3", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

---tabs---

local PlayerTab = Window:MakeTab({
	Name = "Player setting",
	Icon = "rbxassetid://7734053495",
	PremiumOnly = false
})

local Section = PlayerTab:AddSection({
	Name = "Setting"
})

---tab 2---

local OtherTab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://7733964640",
	PremiumOnly = false
})

local Section = OtherTab:AddSection({
	Name = "Very cool scripts"
})

---tabs 3---

local SlimesTab = Window:MakeTab({
	Name = "Revenge Of Slimes",
	Icon = "rbxassetid://7743866529",
	PremiumOnly = false
})

local Section = SlimesTab:AddSection({
	Name = "Cool infinity coins"
})

---Other tabs---

OtherTab:AddButton({
	Name = "no cooldown",
	Callback = function()
      		loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-no-cooldown-6517"))()
  	end    
})

--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]

