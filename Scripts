--[[
Name = <string> - The name of the button.
Callback = <function> - The function of the button.
]]



---Player Tabs---

PlayerTab:AddButton({
	Name = "WalkSpeed",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 125
  	end    
})

PlayerTab:AddButton({
	Name = "JumpPower",
	Callback = function()
      		game.Players.LocalPlayer.Character.Humanoid.JumpPower = 475
  	end    
})

PlayerTab:AddButton({
	Name = "Infinity Jump",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/g76cP72T"))()
  	end    
})

PlayerTab:AddButton({
	Name = "Esp health bar",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/PjjC3SL3"))()
  	end    
})

---Slimes Tabs---

SlimesTab:AddButton({
	Name = "1 Trillions Coins",
	Callback = function()
      		game.Workspace.DoShopPurchase:InvokeServer(game.Players.LocalPlayer.leaderstats.Coins, -1000000000000,"skill3")
  	end    
})

SlimesTab:AddButton({
	Name = "100 Billions Coins",
	Callback = function()
      		game.Workspace.DoShopPurchase:InvokeServer(game.Players.LocalPlayer.leaderstats.Coins, -100000000000,"skill3")
  	end    
})

SlimesTab:AddButton({
	Name = "10 Billions Coins",
	Callback = function()
      		game.Workspace.DoShopPurchase:InvokeServer(game.Players.LocalPlayer.leaderstats.Coins, -10000000000,"skill3")
  	end    
})

SlimesTab:AddButton({
	Name = "1 Billions Coins",
	Callback = function()
      		game.Workspace.DoShopPurchase:InvokeServer(game.Players.LocalPlayer.leaderstats.Coins, -1000000000,"skill3")
  	end    
})
